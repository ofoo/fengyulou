<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.guoguo.fengyulou.dao.task.user.TaskUserDao">

    <insert id="insertTaskUserBatch">
        insert into fengyulou_task_user(task_id,user_id,disabled) values
        <foreach collection="list" item="item" separator=",">
            (
            #{item.taskId},
            #{item.userId},
            #{item.disabled}
            )
        </foreach>
    </insert>
</mapper>